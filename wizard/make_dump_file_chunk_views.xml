<?xml version="1.0"?>
<odoo>
        <!-- Merge Opportunities  -->
        <record id="tender_cat_make_dump_file_chunk" model="ir.ui.view">
            <field name="name">tender_cat.make.dump.file.chunk.form</field>
            <field name="model">tender_cat.make.dump.file.chunk</field>
            <field name="arch" type="xml">
                <form string="Make dump for data model">
                    <group >
						<field name="data_model_id" />
					</group>
					<group >
                        <field name="user_id" class="oe_inline"/>
                    </group>
                    <footer>
                        <button name="action_make_dump" type="object" string="Make dump" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <act_window
            id="action_tender_cat_make_dump_file_chunk"
            name="Make dump for data models"
            res_model="tender_cat.make.dump.file.chunk"
            binding_model="tender_cat.file_chunk"
            binding_views="list"
            view_mode="form"
            target="new"
		/>

</odoo>
